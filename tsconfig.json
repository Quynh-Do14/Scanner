{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl":".",
    "typeRoots": ["src/types"],
    "paths": {
      "@/components":["src/components"],
      "@/config":["src/config"],
      "@/constants":["src/constants"],
      "@/hooks":["src/hooks"],
      "@/locales":["src/locales"],
      "@/navigation" :["src/navigation"],
      "@/redux":["src/redux"],
      "@/screens":["src/screens"],
      "@/services":["src/services"],
      "@/styles":["src/styles"],
      "@/types":["src/types"],
      "@/utils":["src/utils"]
    }
  }
}
