{
  "name": "document",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "a": "react-native run-android",
    "i": "react-native run-ios",
    "apk": "cd android &&./gradlew clean && ./gradlew assembleRelease && cd ..",
    "app-apk": "cd android &&./gradlew app:assembleRelease && cd ..",
    "variant": "react-native run-android --variant=release",
    "rm-apk": "adb shell pm uninstall --user 0 com.document",
    "clear-cache": "adb shell pm clear com.document",
    "rm": "rm -rf ./node_modules",
    "rm-pod": "cd ios && rm Podfile.lock",
    "pod": "npx pod-install",
    "asset": " npx react-native-asset",
    "upgrade": "npx react-native upgrade"
  },
